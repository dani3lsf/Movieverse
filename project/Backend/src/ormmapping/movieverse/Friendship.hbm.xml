<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="movieverse.Friendship" table="Friendship" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="date" column="`Date`" type="date" not-null="false" lazy="false"/>
		<many-to-one name="ORM_ReceivedMuser" cascade="lock" column="MUserId" class="movieverse.MUser" not-null="true" lazy="false">
		</many-to-one>
		<many-to-one name="ORM_RequestedMuser" cascade="lock" column="MUserId2" class="movieverse.MUser" not-null="true" lazy="false">
		</many-to-one>
	</class>
</hibernate-mapping>
